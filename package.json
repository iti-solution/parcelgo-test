{
  "name": "v1",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "render-build": "npm install --production && npm install sharp@latest --platform=linux --arch=x64 && NODE_OPTIONS=--no-deprecation next build",
    "render-start": "NODE_OPTIONS=--no-deprecation next start -p 1000"
  },
  "dependencies": {
    "@payloadcms/db-postgres": "3.32.0",
    "@payloadcms/next": "3.32.0",
    "@payloadcms/payload-cloud": "3.32.0",
    "@payloadcms/richtext-lexical": "3.32.0",
    "cross-env": "^7.0.3",
    "framer-motion": "^12.4.2",
    "graphql": "^16.8.1",
    "hamburger-react": "^2.5.2",
    "loader-utils": "^3.3.1",
    "next": "^15.2.4",
    "next-intl": "^4.0.2",
    "next-themes": "^0.4.4",
    "payload": "^3.32.0",
    "rc-pagination": "^5.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.4.0",
    "react-textarea-autosize": "^8.5.7",
    "react-turnstile": "^1.1.4",
    "sass": "^1.84.0",
    "sharp": "0.32.6",
    "swiper": "^11.2.3",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@types/node": "^22.5.4",
    "@types/react": "19.0.7",
    "@types/react-dom": "19.0.3",
    "eslint": "^9.16.0",
    "eslint-config-next": "^15.2.4",
    "prettier": "^3.4.2",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ]
  }
}
